container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python pathfinder/manage.py migrate --noinput"
option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "pathfinder.settings.production"
    "PYTHONPATH": "/opt/python/current/app/pathfinder:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: pathfinder/pathfinder/wsgi.py
